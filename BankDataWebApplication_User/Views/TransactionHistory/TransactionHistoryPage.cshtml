@{
}
<div id="TransactionHistoryPage">
    <h2><b><u>Transaction History</u></b></h2>
    <div>
        <label for="StartDateInputID">Start Date:</label>
        <input type="date" id="StartDateInputID" name="StartDate" />

        <label for="EndDateInputID">End Date:</label>
        <input type="date" id="EndDateInputID" name="EndDate" />

        <button type="button" id="FilterID" onclick="loadTransactionHistory()">Filter</button>
    </div>

    <table border="2" id="TransactionHistoryTable">
        <thead>
            <tr>
                <th>Date</th>
                <th>Description</th>
                <th>Amount</th>
                <th>Type</th>
            </tr>
        </thead>
        <tbody id="TransactionHistoryTableBody">
            <!-- Transaction data will be inserted here by JavaScript -->
        </tbody>
    </table>
    <div id="NoTransactionsMessage" style="display: none;">
        No transactions found for the selected date range.
    </div>
</div>

<script>
    // Load transaction history when the page loads
    document.addEventListener('DOMContentLoaded', loadTransactionHistory);
</script>